<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<?import javafx.scene.control.ListView?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AdminController">
    <children>
        <Button fx:id="pauseButton" layoutX="227.0" layoutY="30.0" mnemonicParsing="false" onAction="#deleteUser" text="Delete" />
        <Button fx:id="exitButton" layoutX="227.0" layoutY="65.0" mnemonicParsing="false" onAction="#handleBack" prefHeight="25.0" prefWidth="47.0" text="Back" />
        <ListView fx:id="allUsers" layoutX="14.0" layoutY="55.0" prefHeight="200.0" prefWidth="200.0" onMouseClicked="#handleMouseClick"/>

        <ImageView fx:id="imageView" fitHeight="150.0" fitWidth="200.0" layoutX="14.0" layoutY="80.0" pickOnBounds="true" preserveRatio="true" />
        <Label prefHeight="17.0" prefWidth="47.0" text="Score:" />
    </children>
</AnchorPane>
